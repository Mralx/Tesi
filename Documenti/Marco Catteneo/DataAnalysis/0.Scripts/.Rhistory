#1.Data input
preserve_summary = read.table("../ProactiveReserve/ProactiveReserveSummary.txt")
pbuddy_summary = read.table("../ProactiveBuddySystem/ProactiveBuddySummary.txt")
side_summary = read.table("../SideFollower/SideSummary.txt")
direct_summary = read.table("../DirectOptimization/DirectSummary.txt")
library(plotly)
ENV_COUNT = 6
for(i in 1:ENV_COUNT){
current_title = paste("Environment",i)
p = plot_ly(direct_summary, x = ~direct_summary$Team_size[Environment==i], y = ~direct_summary$Avg_exp_time[Environment==i],
type = 'scatter',
mode = 'lines+markers',
name = 'Direct optimization',
line = list(color = 'rgb(255, 187, 0)')) %>%
add_trace(y = ~preserve_summary$Avg_exp_time[Environment==i], name = 'Proactive Reserve', line = list(color = 'rgb(0, 8, 255)', width = 1, dash='dot')) %>%
add_trace(y = ~pbuddy_summary$Avg_exp_time[Environment==i], name = 'Proactive Buddy', line = list(color = 'rgb(255, 0, 0)', width = 1, dash='dot')) %>%
add_trace(y = ~side_summary$Avg_exp_time[Environment==i], name = 'Side Follower', line = list(color = 'rgb(170, 11, 109)', width = 1, dash='dot')) %>%
layout(title = current_title,
xaxis = list(title = "Team size [number of robots]"),
yaxis = list (title = "Exploration time [seconds]"))
print(p)
}
